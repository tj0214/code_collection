//폴더 복사
cp -r 원본폴더 목적지폴더

//서버 용량 확인
df -m

//서버 강제종료
fuser -k 8080/tcp

//로그실시간 보기
tail -f 파일명

//로그보기
vi 파일명

//파일 실행
./파일명

//파일 삭제
rm 파일명

//디렉토리 삭제
rm -r 디렉토리명

//검색
grep -r -i "color\|size" | cut -d ":" -f2 | sort | uniq -c
-r 하위 디렉토리
-i 대소문자 구분 x
-e 정규식
-f 정규식 x

//출력 전체 무시
>/dev/null 2>&1

//오류 출력만 무시
2>/dev/null

//표준 출력만 무시
1>/dev/null

//alias
alias ll='ls -alF'
alias f='find . -iname'
alias g='grep -r -i'
alias gcl='gcloud auth list'
alias q='bq query --nouse_legacy_sql'
alias time='export TMOUT=0'
alias test='bash /home/test/shell/test.sh'

//서버죽이기
netstat -a -o

taskkill /f /pid 12345

//ip확인
ipconfig

//실시간 로그확인
tail -f 로그파일

//상위폴더로 이동
cd ..

//파일실행
./파일명

//현위치 파일목록
ll

